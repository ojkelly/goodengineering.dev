{
  "name": "goodengineering.dev",
  "description": "Stories from the top engineering blogs in one place",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "prebuild": "yarn feeds",
    "build-debug": "NODE_ENV=development; gatsby build --no-uglify",
    "serve": "NODE_ENV=production; gatsby serve",
    "clean": "rm -rf .cache && yarn start",
    "dev": "nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "lint": "eslint . --ext ts --ext tsx",
    "prettier-write": "prettier --write \"src/**/*.js\"",
    "release": "auto shipit",
    "start": "yarn dev",
    "feeds": "ts-node --project feeds/tsconfig.json feeds/index.ts",
    "clean-feeds": "cat remove.txt |  while read x ; do sed -i ''  '\\|\"'$x'\"|d' source.opml ; done "
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn prettier-write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "prettier": {
    "bracketSpacing": true,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
